<p id="notice"><%= notice %></p>

<h1>Lista de Tipo Medicos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_medicos.each do |tipo_medico| %>
      <tr>
        <td><%= tipo_medico.nombre %></td>
        <td><%= link_to 'Show', tipo_medico %></td>
        <td><%= link_to 'Edit', edit_tipo_medico_path(tipo_medico) %></td>
        <td><%= link_to 'Destroy', tipo_medico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Tipo medico', new_tipo_medico_path %>
